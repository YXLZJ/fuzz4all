#include <iostream>
#include <string>
#include <vector>
#include <algorithm>

int main() {
    std::vector<std::string> words = {"apple", "banana", "cherry"};
    
    auto sort_words = [](std::vector<std::string>& v) {
        std::sort(v.begin(), v.end());
    };
    
    sort_words(words);
    
    for (const auto& word : words) {
        std::cout << word << ' ';
    }
    
    return 0;
}