#include <iostream>
#include <string>
#include <vector>
#include <algorithm>

int main() {
    std::vector<std::string> words = {"hello", "world"};
    auto const_words = [&](const std::vector<std::string>& w) { return w; };
    
    for (auto word : const_words(words)) {
        std::cout << word << ' ';
    }
    
    return 0;
}