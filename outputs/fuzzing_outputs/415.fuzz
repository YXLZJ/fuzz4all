#include <iostream>
#include <string_view>
#include <vector>
#include <algorithm>

int main() {
    std::vector<std::string> words = {"apple", "banana", "cherry"};
    
    auto length_comparison = [](const std::string& a, const std::string& b) { return a.length() < b.length(); };
    std::sort(words.begin(), words.end(), length_comparison);

    for (auto word : words) {
        std::cout << word << " ";
    }
    
    return 0;
}