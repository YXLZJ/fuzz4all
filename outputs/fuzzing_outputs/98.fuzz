#include <iostream>
using namespace std;

int main() {
    auto lambda = [](auto&&... args) -> decltype(args.first * args.second) { return (args.first * args.second); };
    
    map<string, pair<double, double>> data{{"a",{10.5, 2}}, {"b",{3.7, 4}}};
    auto result = accumulate(data.begin(), data.end(), 0.0, lambda);
    
    cout << "Result: " << result << endl;
    
    return 0;
}