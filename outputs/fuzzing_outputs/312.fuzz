#include <iostream>
#include <vector>
#include <algorithm>
using namespace std;

int main() {
    vector<double> data = {102., 4.};
    
    sort(data.begin(), data.end());
    
    cout << "Sorted: ";
    for (const auto& value : data) {
        cout << value << ' ';
    }
    cout << '\n';
  
    return 0;
}