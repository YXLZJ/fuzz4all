#include <iostream>
#include <random>
int main() {
    std::mt19937 rng(std::random_device{}());
    std::uniform_real_distribution<double> dist(-5, 5);
    
    for (int i = 0; i < 10; ++i) {
        double randomNumber = dist(rng);
        std::cout << "Random number: " << randomNumber << '\n';
    }

    return 0;}