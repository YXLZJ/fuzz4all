#include <iostream>
#include <random>
int main(int argc, char* argv[]) {
    std::mt19937 rng(std::random_device{}());
    std::uniform_int_distribution<int> dist(0, 9);
    
    for (int i = 0; i < 5; ++i) {
        int randomNumber = dist(rng);
        std::cout << "Random number: " << randomNumber << '\n';
    }
    
    return 0;
}