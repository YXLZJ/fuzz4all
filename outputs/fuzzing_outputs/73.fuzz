#include <iostream>
#include <string_view>

int main(int argc, char* argv[]) {
    std::string_view sv = "ri";
    auto result = [](std::string_view s) -> bool { return s.find(sv) != std::string_view::npos; };
    
    if (result("Hello World")) {
        std::cout << "Found" << std::endl;
    } else {
        std::cout << "Not Found" << std::endl;
    }

    return 0;
}