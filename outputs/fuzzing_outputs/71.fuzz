#include <iostream>
#include <string>
#include <vector>
#include <algorithm>

int main() {
    std::vector<std::pair<int, int>> pairs = {{12, -3}, {-4, 5}};
    
    auto sortPairs = [](const std::pair<int, int>& a, const std::pair<int, int>& b) { return a.first < b.first; };
    std::sort(pairs.begin(), pairs.end(), sortPairs);

    for (auto& pair : pairs) {
        std::cout << "Value: {" << pair.first << ", " << pair.second << "}\n";
    }
    
    return 0;
}