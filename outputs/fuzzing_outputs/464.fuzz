#include <iostream>
#include <vector>
#include <string>

int main() {
    // Using C++17 structured binding to unpack a tuple
    auto [name, age] = std::make_tuple("John Doe", 30);
    
    // Printing the name and age using fold expression in C++17
    (std::cout << ... ) << "Name: " << name << ", Age: " << age;
    
    return 0;
}