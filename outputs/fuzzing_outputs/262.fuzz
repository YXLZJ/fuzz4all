#include <iostream>
#include <vector>
#include <algorithm>
#include <execution>

int main() {
    std::vector<int> arr = {1, 2, 3, 4, 5};
    
    // Using parallel execution policy to sort the vector in-place
    std::sort(std::execution::par, arr.begin(), arr.end());
    
    for (const auto& val : arr) {
        std::cout << val << " ";
    }
    
    return 0;
}