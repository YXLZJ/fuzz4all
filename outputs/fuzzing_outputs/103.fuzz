#include <iostream>
#include <vector>
#include <algorithm>

int main() {
    std::vector<double> data = {1, 2, 3, 4, 5};
    
    auto sum_of_squares = [](const std::vector<double>& v) {
        return std::inner_product(v.begin(), v.end(), v.begin(), 0);
    };

    double result = sum_of_squares(data);
    
    std::cout << "Sum of squares: " << result << std::endl;

    return 0;
}