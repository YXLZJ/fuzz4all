#include <iostream>
#include <string>
#include <vector>
#include <algorithm>
#include <iterator>

int main() {
    std::vector<std::string> words = {"apple", "banana", "cherry"};
    
    // Use range-based for loop to iterate over the vector elements.
    for (const auto& word : words) {
        std::cout << word << ' ';
    }
    
    return 0;
}