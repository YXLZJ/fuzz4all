#include <iostream>
#include <vector>
#include <algorithm>
#include <string>
using namespace std;

int main() {
    vector<string> words = {"ora", "ira"};
    sort(words.begin(), words.end());
    
    for (const auto& word : words) {
        reverse(word.begin(), word.end());
        cout << word << endl;
    }
}