#include <stdio.h>

int main() {
    int num;
    printf("Enter a number: ");
    scanf("%d", &num);

    if (num > 0) {
        goto positive;
    } else if (num < 0) {
        goto negative;
    }

positive:
    printf("The number is positive.\n");
    return 0;

negative:
    printf("The number is negative.\n");
    return 0;
}