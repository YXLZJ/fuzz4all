#include <iostream>
#include <vector>
#include <algorithm>

int main() {
    std::vector<std::vector<double>> data = {{1,2,3}, {4,5,6}};
    
    auto bound_u = [](const std::vector<double>& v) -> double {
        return *std::max_element(v.begin(), v.end());
    };

    for (auto& row : data) {
        std::cout << "Bound: " << bound_u(row) << '\n';
    }
    
    return 0;
}