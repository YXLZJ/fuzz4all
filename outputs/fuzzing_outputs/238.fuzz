#include <iostream>
#include <vector>
#include <algorithm>
using namespace std;

int main() {
    vector<double> data = {1.2, 3.4, 5.6, 7.8};
    
    auto sort_descending = [](double a, double b) { return a > b; };
    sort(data.begin(), data.end(), sort_descending);
    
    for (const auto& d : data) {
        cout << d << " ";
    }
    
    return 0;
}