#include <iostream>
using namespace std;

int main() {
    auto lambda = [](auto x){ return x * x; };
    cout << "Lambda function result: " << lambda(5) << endl;
    
    vector<double> v{1.0, 2.0, 3.0};
    for (const auto& elem : v) {
        cout << elem << ' ';
    }
    cout << endl;

    return 0;
}