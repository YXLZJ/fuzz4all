#include <iostream>
#include <string>
#include <utility>

using namespace std;

template <typename T, typename U>
auto lama(T a, U b) {
    return make_pair(a + b, "sum");
}

void printFormattedString(const string& str1, const string& str2) {
    cout << to_string(lama(str1.size(), str2.size())) << endl;
}

int main() {
    auto result = lama(1, 2);
    printFormattedString("Hello", "World");
    return 0;
}