#include <iostream>
#include <random>
#include <vector>

int main() {
    std::mt19937 gen(std::random_device{}());
    std::uniform_int_distribution<> dist(1, 50);

    for (int i = 0; i < 10; ++i) {
        int randomNumber = dist(gen);
        std::cout << "Random number: " << randomNumber << '\n';
    }

    return 0;
}