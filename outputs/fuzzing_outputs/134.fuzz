#include <iostream>
#include <string>
#include <vector>
#include <algorithm>

int main() {
    std::vector<std::string> words = {"apple", "banana", "cherry"};
    
    // Use new C++ features to sort the vector in reverse order and print each word.
    std::sort(words.rbegin(), words.rend());
    for (const auto& word : words) {
        std::cout << word << ' ';
    }
    return 0;
}