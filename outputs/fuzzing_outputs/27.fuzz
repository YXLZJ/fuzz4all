#include <stdio.h>

int main() {
    int num;
    printf("Enter a number: ");
    scanf("%d", &num);

    if (num > 0) {
        goto positive;
    } else if (num < 0) {
        goto negative;
    } else {
        goto zero;
    }

positive:
    printf("Number is positive.\n");
    return 0;

negative:
    printf("Number is negative.\n");
    return 0;

zero:
    printf("Number is zero.\n");
    return 0;
}