#include <iostream>
#include <vector>
#include <algorithm>

int main() {
    std::vector<std::string> words = {"apple", "banana", "cherry"};
    
    // Use lambda function to sort the vector in reverse order
    auto compare = [](const std::string& a, const std::string& b) { return a > b; };
    std::sort(words.begin(), words.end(), compare);

    for (const auto& word : words) {
        std::cout << word << ' ';
    }
    
    return 0;
}