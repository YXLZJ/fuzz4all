#include <iostream>
#include <string>
#include <vector>
#include <algorithm>

int main() {
    std::vector<std::string> words = {"Hello", "World", "C++", "Programming"};
    
    // Use range-based for loop (new C++ feature)
    for(const auto& word : words) {
        std::cout << word << ' ';
    }
    
    return 0;
}