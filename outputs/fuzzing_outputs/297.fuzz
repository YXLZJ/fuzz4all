#include <iostream>
#include <vector>
#include <algorithm>
using namespace std;

int main() {
    vector<string> words = {"Hello", "World"};
    
    // Use C++17 structured binding to unpack the pair returned by transform_view
    for (auto [word, index] : subrange(words)) {
        cout << word << ": " << index << endl;
    }

    return 0;
}