#include <iostream>
#include <vector>
#include <algorithm>

int main() {
    std::vector<std::string> words = {"apple", "banana", "cherry", "date"};
    
    auto sort_words = [](const std::string& a, const std::string& b) { return a.size() < b.size(); };
    std::sort(words.begin(), words.end(), sort_words);

    for (const auto& word : words) {
        std::cout << word << " ";
    }
    
    return 0;
}