#include <iostream>
#include <vector>
#include <algorithm>
#include <execution>

int main() {
    std::vector<int> v = {1, 2, 3, 4, 5};
    
    // Using parallel execution policy to sort the vector in-place
    std::sort(std::execution::par, v.begin(), v.end());

    for (const auto& i : v) {
        std::cout << i << " ";
    }

    return 0;
}