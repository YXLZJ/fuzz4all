#include <iostream>
#include <string>
#include <vector>
#include <algorithm>

int main() {
    std::vector<std::string> words = {"complex", "way", "features"};
    
    auto sorted_words = [](const std::string& a, const std::string& b) { return a.size() > b.size(); };
    std::sort(words.begin(), words.end(), sorted_words);
    
    for (const auto& word : words) {
        std::cout << word << " ";
    }
    
    return 0;
}