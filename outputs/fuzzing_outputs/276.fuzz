#include <iostream>
#include <string>
#include <vector>
#include <algorithm>
using namespace std;

int main() {
    vector<string> words = {"apple", "banana", "cherry", "date"};
    
    // Use new C++ features to sort the vector in reverse order and print it.
    sort(words.rbegin(), words.rend());
    
    for (const auto& word : words) {
        cout << word << endl;
    }
    
    return 0;
}