#include <iostream>
#include <string>
using namespace std;

int main() {
    string input = "a";
    auto result = [](const string& s) -> bool { return !s.empty(); };
    
    if (result(input)) {
        cout << "Input is not empty." << endl;
    } else {
        cout << "Input is empty." << endl;
    }

    return 0;
}