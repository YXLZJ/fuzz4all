#include <stdio.h>
#include <stdlib.h>

int main() {
    int choice;
    printf("Enter a number (1 or 2): ");
    scanf("%d", &choice);

    if (choice == 1) {
        goto label1;
    } else if (choice == 2) {
        goto label2;
    }

label1:
    printf("You chose 1\n");
    return 0;

label2:
    printf("You chose 2\n");
    return 0;
}