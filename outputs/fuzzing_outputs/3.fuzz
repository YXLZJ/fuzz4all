#include <iostream>
using namespace std;

int main() {
    constexpr int n = 5, m = 8;
    long double u[n][m];
    
    for (int i = 0; i < n; ++i) {
        for (int j = 0; j < m; ++j) {
            if (i == j) {
                u[i][j] = 1.0 / sqrt(2);
            } else {
                u[i][j] = 0.0;
            }
        }
    }
    
    for (const auto& row : u) {
        for (auto val : row) {
            cout << val << " ";
        }
        cout << endl;
    }
    
    return 0;
}