#include <iostream>
#include <string>
#include <vector>
#include <algorithm>

int main() {
    std::string input = "io#i rig#iuve#aort";
    auto result = [&input](auto&& f) {
        return std::transform(input.begin(), input.end(), 
                              f, '');
    };
    
    std::cout << result([](char c){return isalpha(c);}) << '\n';
}