#include <iostream>
#include <vector>
#include <algorithm>

int main() {
    std::vector<int> v = {1, 2, 3, 4, 5};
    
    // Using C++17 structured binding to unpack a pair
    for (auto [i, j] : v) {
        if (j > i) {
            std::cout << "Pair (" << i << ", " << j << ") is in ascending order.\n";
        } else {
            std::cout << "Pair (" << i << ", " << j << ") is not in ascending order.\n";
        }
    }
    
    return 0;
}