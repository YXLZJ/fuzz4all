#include <iostream>
#include <random> // Include the necessary header for random number generation

int main() {
    std::mt19937_64 rng(std::random_device{}()); // Seed with a random device
    std::uniform_int_distribution<long long int> dist(0, 100); // Define the distribution range

    for (int i = 0; i < 5; ++i) {
        std::cout << "Random number: " << dist(rng) << std::endl; // Generate and print a random number
    }

    return 0;
}