#include <iostream>
#include <ranges>
int main() {
    std::vector<int> v = {1, 2, 3, 4, 5};
    auto result = v | std::views::transform([](auto x) { return x * x; })
                   | std::views::filter([](auto x) { return x > 10; });
    
    for (int i : result) {
        std::cout << i << " ";
    }
    std::cout << '\n';
}