#include <stdlib.h>

int main() {
    int n = 5;
    int arr[n];
    for (int i = 0; i < n; ++i) {
        arr[i] = rand();
        if (arr[i] % 2 == 0) {
            goto even;
        }
    }
    printf("No even number found\n");
    return 0;

even:
    printf("First even number: %d\n", arr[i]);
    return 0;
}