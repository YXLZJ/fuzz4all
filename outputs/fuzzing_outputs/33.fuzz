#include <iostream>
using namespace std;

int main() {
    int i, j, k;
    auto [i, j] = to_array(10); // Using C++20 feature: structured bindings with std::to_array
    
    for (k = 0; k < 5; ++k) {
        cout << "Iteration " << k + 1 << ": i=" << i << ", j=" << j << endl; // Using C++17 feature: '<<' operator chaining
    }
    
    return 0;
}