#include <iostream>
#include <string_view>
#include <optional>
#include <variant>

int main() {
    std::optional<std::variant<int, double>> value = std::nullopt;
    
    if (value.has_value()) {
        int iValue = std::get<0>(value);
        std::cout << "Integer: " << iValue << '\n';
    } else {
        std::string_view message = "No value present";
        std::cout << message;
    }
    
    return 0;
}