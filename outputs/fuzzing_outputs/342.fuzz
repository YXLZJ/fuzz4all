#include <iostream>
#include <vector>
#include <numeric>
#include <algorithm>

int main() {
    std::vector<int> v = {1, 2, 3, 4};
    int sum = std::accumulate(v.begin(), v.end(), 0);
    
    for (auto elem : v) {
        std::cout << elem << ";";
    }
    
    std::cout << "\nSum: " << sum;
    
    return 0;}